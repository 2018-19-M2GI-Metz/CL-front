{
  "name": "cl-front",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve -o",
    "start:mock": "ng serve -c mock -o",
    "build": "ng build",
    "build:mock": "ng build -c mock",
    "build:prod:mock": "ng build -c mock:production && npm run prepare",
    "build:prod": "ng build --prod && npm run prepare",
    "prepare": "cd cordova && cordova prepare",
    "build:apps": "cd cordova && cordova build --release",
    "start:electron": "cd cordova && cordova run cordova-electron",
    "start:android": "cd cordova & cordova run android",
    "lint": "ng lint --fix",
    "generate:doc": "compodoc -p src/tsconfig.app.json"
  },
  "private": true,
  "main": "main.js",
  "dependencies": {
    "@angular/animations": "^7.2.8",
    "@angular/cdk": "^7.3.3",
    "@angular/common": "~7.2.8",
    "@angular/compiler": "~7.2.8",
    "@angular/core": "~7.2.8",
    "@angular/forms": "~7.2.8",
    "@angular/http": "~7.2.8",
    "@angular/material": "^7.3.3",
    "@angular/platform-browser": "~7.2.8",
    "@angular/platform-browser-dynamic": "~7.2.8",
    "@angular/router": "~7.2.8",
    "core-js": "^2.6.5",
    "mappa-mundi": "0.0.5",
    "rxjs": "~6.4.0",
    "zone.js": "~0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.5",
    "@angular/cli": "~7.3.5",
    "@angular/compiler-cli": "~7.2.8",
    "@angular/language-service": "~7.2.8",
    "@compodoc/compodoc": "^1.1.9",
    "@types/jasmine": "~3.3.9",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~11.11.0",
    "codelyzer": "~4.5.0",
    "helloworld": "file:cordova",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.2",
    "ts-node": "~8.0.3",
    "tslint": "~5.13.1",
    "typescript": "~3.2.4"
  }
}